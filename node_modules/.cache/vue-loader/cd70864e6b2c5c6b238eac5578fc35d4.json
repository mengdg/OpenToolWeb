dropdown>\n</template>\n\n<script>\nexport default {\n  props: {\n    value: {\n      required: true,\n      default: () => [],\n      type: Array\n    },\n    project: {\n      type: String,\n      default: ''\n    }\n  },\n  data() {\n    return {}\n  },\n  computed: {\n    selectProject: {\n      get() {\n        return this.project\n      },\n      set(val) {\n        this.value.forEach(element => {\n          if (element.label === val) {\n            this.$emit('input', element)\n          }\n        })\n      }\n    }\n  },\n  mounted() {}\n}\n</script>\n"]}]}