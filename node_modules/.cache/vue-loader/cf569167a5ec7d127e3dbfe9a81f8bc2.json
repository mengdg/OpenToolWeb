{"remainingRequest":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/mengdegong/Desktop/blued/new/vue-element-admin/src/views/tools/components/EditPage.vue?vue&type=style&index=0&id=2fc5be58&lang=scss&scoped=true&","dependencies":[{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/src/views/tools/components/EditPage.vue","mtime":1614932856621},{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/sass-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/mengdegong/Desktop/blued/new/vue-element-admin/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["EditPage.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6YA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"EditPage.vue","sourceRoot":"src/views/tools/components","sourcesContent":["<template>\n  <div class=\"createPost-container\">\n    <el-form ref=\"postForm\" :model=\"postForm\" :rules=\"rules\" class=\"form-container\">\n\n      <sticky :z-index=\"10\" class-name=\"sub-navbar draft\">\n        <!-- <OperateDropdown v-model=\"postForm.platforms\" />\n        <el-button v-loading=\"loading\" style=\"margin-left: 10px;\" type=\"success\" @click=\"submitForm\">\n          Add Func\n        </el-button> -->\n        <el-button v-loading=\"loading\" type=\"warning\" @click=\"draftForm\">\n          Save\n        </el-button>\n      </sticky>\n\n      <div class=\"createPost-main-container\">\n\n        <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\">\n          <el-tab-pane label=\"基本信息\" name=\"first\">\n            <el-row>\n              <el-form-item style=\"margin-top: 20px;\" prop=\"title\">\n                <el-input v-model=\"postForm.title\" placeholder=\"请输入工具名称\">\n                  <template slot=\"prepend\">\n                    名称\n                  </template>\n                </el-input>\n              </el-form-item>\n\n              <el-form-item prop=\"image_uri\">\n                <el-input v-model=\"postForm.image_uri\" placeholder=\"请输入工具图标链接\">\n                  <template slot=\"prepend\">\n                    图标\n                  </template>\n                </el-input>\n              </el-form-item>\n\n              <el-form-item style=\"margin-bottom: 40px;\" prop=\"content_short\">\n                <el-input v-model=\"postForm.content_short\" :rows=\"4\" type=\"textarea\" placeholder=\"请输入功能介绍\" />\n              </el-form-item>\n\n            </el-row>\n          </el-tab-pane>\n\n          <el-tab-pane label=\"变量管理\" name=\"second\">\n            <el-form-item style=\"margin-top:18px\">\n              <el-button style=\"margin-bottom: 15px\" type=\"primary\" @click=\"handleAddVar\">添加变量</el-button>\n              <el-table\n                ref=\"multipleTable\"\n                :data=\"postForm.vars\"\n                tooltip-effect=\"dark\"\n                style=\"width: 100%\"\n              >\n                <el-table-column\n                  label=\"展示名称\"\n                  width=\"160\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-input v-model=\"scope.row.name\" placeholder=\"请输入显示名称\" />\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  label=\"变量名称\"\n                  width=\"160\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-input v-model=\"scope.row.varName\" placeholder=\"请输入变量名称\" @input=\"checkVar\" />\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  prop=\"name\"\n                  label=\"类型\"\n                  width=\"130\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-select\n                      v-model=\"scope.row.type.value\"\n                      placeholder=\"请选择\"\n                      @change=\"selectChange(scope.row)\"\n                    >\n                      <el-option\n                        v-for=\"item in options\"\n                        :key=\"item.value\"\n                        :label=\"item.label\"\n                        :value=\"item.value\"\n                      />\n                    </el-select>\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  label=\"占位符\"\n                  width=\"160\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-input v-model=\"scope.row.placeholder\" placeholder=\"请输入占位符\" />\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  prop=\"address\"\n                  label=\"默认值\"\n                  show-overflow-tooltip\n                >\n                  <template slot-scope=\"scope\">\n                    <el-input\n                      v-if=\"scope.row.type.value == 'string'\"\n                      v-model=\"scope.row.default\"\n                      :placeholder=\"scope.row.placeholder\"\n                    />\n                    <el-input\n                      v-else-if=\"scope.row.type.value == 'areatext'\"\n                      v-model=\"scope.row.default\"\n                      type=\"textarea\"\n                      :rows=\"1\"\n                      :placeholder=\"scope.row.placeholder\"\n                    />\n                    <el-input-number\n                      v-else-if=\"scope.row.type.value == 'number'\"\n                      v-model=\"scope.row.default\"\n                      :placeholder=\"scope.row.placeholder\"\n                    />\n                    <el-switch\n                      v-else-if=\"scope.row.type.value == 'boolean'\"\n                      v-model=\"scope.row.default\"\n                      active-color=\"#13ce66\"\n                      inactive-color=\"#ff4949\"\n                    />\n                    <el-date-picker\n                      v-else-if=\"scope.row.type.value == 'time'\"\n                      v-model=\"scope.row.default\"\n                      type=\"datetime\"\n                      value-format=\"yyyy-MM-dd hh:mm:ss\"\n                      :placeholder=\"scope.row.placeholder\"\n                    />\n                    <el-select\n                      v-else-if=\"scope.row.type.value === 'radio' || scope.row.type.value === 'checkbox' || scope.row.type.value === 'dropdown'\"\n                      v-model=\"scope.row.default\"\n                      multiple\n                      filterable\n                      allow-create\n                      default-first-option\n                      :placeholder=\"scope.row.placeholder\"\n                      style=\"width:100%\"\n                    />\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  prop=\"address\"\n                  label=\"操作\"\n                  show-overflow-tooltip\n                  style=\"float:right\"\n                  width=\"80\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-button\n                      type=\"danger\"\n                      size=\"small\"\n                      @click=\"handleDeleteVar(scope.$index)\"\n                    >\n                      删除\n                    </el-button>\n                  </template>\n                </el-table-column>\n              </el-table>\n            </el-form-item>\n          </el-tab-pane>\n\n          <el-tab-pane label=\"code review\" name=\"third\">\n            <el-form-item style=\"margin-top:18px\">\n              <AceCode :value=\"postForm.code\" @input=\"changeCodeInput\" />\n            </el-form-item>\n          </el-tab-pane>\n\n          <el-tab-pane label=\"调用函数\" name=\"fourth\">\n            <el-form-item style=\"margin-top:18px\">\n              <el-button style=\"margin-bottom: 15px\" type=\"primary\" @click=\"handleAddFunc\">添加调用操作</el-button>\n              <el-table\n                ref=\"multipleTable\"\n                :data=\"postForm.runs\"\n                tooltip-effect=\"dark\"\n                style=\"width: 100%\"\n              >\n                <el-table-column\n                  label=\"名称\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-input v-model=\"scope.row.name\" placeholder=\"请输入名称\" />\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  label=\"关键字\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-input v-model=\"scope.row.script\" placeholder=\"请输入关键字\" />\n                  </template>\n                </el-table-column>\n\n                <el-table-column\n                  prop=\"address\"\n                  label=\"操作\"\n                  show-overflow-tooltip\n                  style=\"float:right\"\n                  width=\"80\"\n                >\n                  <template slot-scope=\"scope\">\n                    <el-button\n                      type=\"danger\"\n                      size=\"small\"\n                      @click=\"handleDeleteFunc(scope.$index)\"\n                    >\n                      删除\n                    </el-button>\n                  </template>\n                </el-table-column>\n              </el-table>\n            </el-form-item>\n            <Warning />\n          </el-tab-pane>\n        </el-tabs>\n\n      </div>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport Warning from './Warning'\n\nimport AceCode from '@/components/ACECode'\nimport Sticky from '@/components/Sticky' // 粘性header组件\n// import draggable from 'vuedraggable'\n\nimport { validURL } from '@/utils/validate'\n\nconst defaultForm = {\n  title: '', // 题目\n  content_short: '', // 摘要\n  image_uri: '', // 图片\n  vars: [],\n  code: '',\n  runs: [],\n  importance: 0\n}\n\nexport default {\n  name: 'ToolEdit',\n  components: { Warning, AceCode, Sticky },\n  data() {\n    const validateRequire = (rule, value, callback) => {\n      if (value === '') {\n        this.$message({\n          message: rule.field + '为必传项',\n          type: 'error'\n        })\n        callback(new Error(rule.field + '为必传项'))\n      } else {\n        callback()\n      }\n    }\n    const validateSourceUri = (rule, value, callback) => {\n      if (value) {\n        if (validURL(value)) {\n          callback()\n        } else {\n          this.$message({\n            message: '外链url填写不正确',\n            type: 'error'\n          })\n          callback(new Error('外链url填写不正确'))\n        }\n      } else {\n        callback(new Error('图标链接为必传项'))\n      }\n    }\n    return {\n      value1: true,\n      postForm: Object.assign({}, defaultForm),\n      rules: {\n        image_uri: [{ validator: validateSourceUri }],\n        title: [{ validator: validateRequire }],\n        content_short: [{ validator: validateRequire }]\n      },\n      loading: false,\n\n      activeName: 'first',\n      varMultipleSelection: null,\n      options: [\n        {\n          value: 'string',\n          label: '字符串'\n        }, {\n          value: 'areatext',\n          label: '文本'\n        }, {\n          value: 'number',\n          label: '数字'\n        }, {\n          value: 'boolean',\n          label: '滑块'\n        }, {\n          value: 'time',\n          label: '时间'\n        }, {\n          value: 'radio',\n          label: '单选'\n        }, {\n          value: 'checkbox',\n          label: '多选'\n        }, {\n          value: 'dropdown',\n          label: '下拉菜单'\n        }\n      ]\n    }\n  },\n  methods: {\n    submitForm() {},\n    handleClick() {},\n    draftForm() {\n      if (this.postForm.title === '' || this.postForm.content_short === '' || this.postForm.image_uri === '') {\n        this.$message({\n          message: '请完善基础信息',\n          type: 'warning'\n        })\n        this.activeName = 'first'\n      } else if (this.postForm.code === '') {\n        this.$message({\n          message: '请编写code',\n          type: 'warning'\n        })\n        this.activeName = 'third'\n      } else if (this.postForm.runs.length === 0) {\n        this.$message({\n          message: '请完善调用函数',\n          type: 'warning'\n        })\n        this.activeName = 'fourth'\n      } else {\n        console.log('请求创建和修改接口')\n      }\n    },\n    handleAddVar() {\n      this.postForm.vars.push({ name: '', varName: '', type: { value: 'string', label: '字符串' }, default: '', placeholder: '请输入默认值' })\n    },\n    handleDeleteVar(index) {\n      this.postForm.vars.splice(index, 1)\n    },\n    checkVar(value) {\n      const reg = /^[a-zA-Z][a-zA-Z0-9_]*$/\n      if (!value) {\n        this.$message({\n          message: '变量名称不能为空',\n          type: 'error'\n        })\n      } else {\n        setTimeout(() => {\n          if (new RegExp(reg).test(value) === false) {\n            this.$message({\n              message: '变量名称只能为数字、字母、下划线',\n              type: 'error'\n            })\n          }\n        }, 1000)\n      }\n    },\n    selectChange(data) {\n      console.log(data.type.value)\n      const dataValue = data.type.value\n      if (dataValue === 'string' || dataValue === 'areatext') {\n        data.default = ''\n      } else if (dataValue === 'number') {\n        data.default = 1\n      } else if (dataValue === 'boolean') {\n        data.default = false\n      } else if (dataValue === 'time') {\n        data.default = ''\n      } else if (dataValue === 'radio' || dataValue === 'checkbox' || dataValue === 'dropdown') {\n        data.default = []\n      }\n    },\n    handleAddFunc() {\n      this.postForm.runs.push({ name: '', script: '' })\n    },\n    handleDeleteFunc(index) {\n      this.postForm.runs.splice(index, 1)\n    },\n    changeCodeInput(code) {\n      this.postForm.code = code\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"~@/styles/mixin.scss\";\n\n.createPost-container {\n  position: relative;\n\n  .createPost-main-container {\n    padding: 40px 45px 20px 50px;\n\n    .postInfo-container {\n      position: relative;\n      @include clearfix;\n      margin-bottom: 10px;\n\n      .postInfo-container-item {\n        float: left;\n      }\n    }\n  }\n\n  .word-counter {\n    width: 40px;\n    position: absolute;\n    right: 10px;\n    top: 0px;\n  }\n}\n</style>\n"]}]}