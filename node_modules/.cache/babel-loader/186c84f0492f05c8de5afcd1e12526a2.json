v-model=\"postForm.content_short\" :rows=\"2\" type=\"textarea\" autosize placeholder=\"请输入功能介绍\" required />\n          </el-form-item>\n\n        </el-row>\n        <el-form-item>\n          <dropdown-menu :items=\"articleList\" title=\"添加功能\" />\n        </el-form-item>\n\n        <el-card>1</el-card>\n        <prism language=\"bash\" :plugins=\"['command-line']\", :code=\"code\" />\n\n      </div>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport Warning from './Warning'\n\nimport DropdownMenu from '@/components/Share/MyDropdownMenu'\nimport MDinput from '@/components/MDinput'\n\nimport { validURL } from '@/utils/validate'\n\nconst defaultForm = {\n  status: 'draft',\n  title: '', // 文章题目\n  content: '', // 文章内容\n  content_short: '', // 文章摘要\n  source_uri: '', // 文章外链\n  image_uri: '', // 文章图片\n  display_time: undefined, // 前台展示时间\n  id: undefined,\n  platforms: ['a-platform'],\n  comment_disabled: false,\n  importance: 0\n}\n\nexport default {\n  name: 'ToolEdit',\n  components: { Warning, DropdownMenu, MDinput },\n  data() {\n    const validateRequire = (rule, value, callback) => {\n      if (value === '') {\n        this.$message({\n          message: rule.field + '为必传项',\n          type: 'error'\n        })\n        callback(new Error(rule.field + '为必传项'))\n      } else {\n        callback()\n      }\n    }\n    const validateSourceUri = (rule, value, callback) => {\n      if (value) {\n        if (validURL(value)) {\n          callback()\n        } else {\n          this.$message({\n            message: '外链url填写不正确',\n            type: 'error'\n          })\n          callback(new Error('外链url填写不正确'))\n        }\n      } else {\n        callback()\n      }\n    }\n    return {\n      postForm: Object.assign({}, defaultForm),\n      articleList: [\n        { title: 'redis', href: 'https://juejin.im/post/59097cd7a22b9d0065fb61d2' },\n        { title: 'mysql', href: 'https://juejin.im/post/591aa14f570c35006961acac' },\n        { title: 'es', href: 'https://juejin.im/post/593121aa0ce4630057f70d35' },\n        { title: '封装类', href: 'https://juejin.im/post/595b4d776fb9a06bbe7dba56' },\n        { title: '自定义脚本', href: 'https://juejin.im/post/5c92ff94f265da6128275a85' }\n      ],\n      rules: {\n        title: [{ validator: validateRequire }],\n        image_uri: [{ validator: validateSourceUri }],\n        content_short: [{ validator: validateRequire }]\n      },\n      code: 'npm install vue-prismjs --save'\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"~@/styles/mixin.scss\";\n\n.createPost-container {\n  position: relative;\n\n  .createPost-main-container {\n    padding: 40px 45px 20px 50px;\n\n    .postInfo-container {\n      position: relative;\n      @include clearfix;\n      margin-bottom: 10px;\n\n      .postInfo-container-item {\n        float: left;\n      }\n    }\n  }\n\n  .word-counter {\n    width: 40px;\n    position: absolute;\n    right: 10px;\n    top: 0px;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/tools/components"}]}