OAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OALA;AAMA;AAhBA;AAhBA,CAAA","sourcesContent":["<template>\n  <div v-loading=\"loading\" class=\"dashboard-container\">\n    <!-- <el-card v-for=\"(item, index) in cards\" :key=\"'top' + index\" class=\"box-card\">\n      <div slot=\"header\" class=\"clearfix\">\n        <span class=\"text text-top\">{{ item.name }}</span>\n        <router-link to=\"/tools/list\">\n          <el-button style=\"float: right; padding: 3px 0\" type=\"text\">更多<i class=\"el-icon-d-arrow-right el-icon--right\" /></el-button>\n        </router-link>\n      </div>\n      <v-top-card :paths=\"item.data\" />\n    </el-card> -->\n    <el-card v-for=\"(item, index) in urlData\" :key=\"index\" class=\"box-card\">\n      <div slot=\"header\" class=\"clearfix\">\n        <span v-if=\"item.name === '推荐应用'\" class=\"text\" style=\"color:red;\">{{ item.name }}</span>\n        <span v-else class=\"text\">{{ item.name }}</span>\n      </div>\n      <v-card :paths=\"item.navs\" />\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport Card from './components/Card'\nimport { Navigation } from '@/api/navigation'\n\nexport default {\n  name: 'Navigation',\n  components: {\n    'v-card': Card\n  },\n  data() {\n    return {\n      cards: [],\n      urlData: [],\n      kinds: [],\n      loading: false\n    }\n  },\n  created() {\n    this._requestNavs()\n  },\n  methods: {\n    _requestNavs() {\n      this.loading = true\n      Navigation.get_kind().then(response => {\n        const { data, code } = response\n        if (code === 200) {\n          this.kinds = data\n        }\n        this.loading = false\n      })\n      Navigation.get_nav().then(response => {\n        const { data } = response\n        this.cards = data\n        this.urlData = data\n        this.loading = false\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.dashboard-container {\n  margin: 24px;\n  min-height: 700px;\n}\n.box-card {\n  width: 100%;\n  margin: 20px 0;\n}\n.text {\n  font-size: 16px;\n  font-weight: 600;\n}\n.text-top {\n  color: red;\n}\n</style>\n\n<style>\n.el-card__header {\n  padding: 12px 14px;\n}\n</style>\n"],"sourceRoot":"src/views/navigation"}]}